{
  "name": "ts-nextjs-tailwind-starter",
  "jest": {
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "license": "SEE LICENSE in LICENSE.md",
  "scripts": {
    "qs": "git status && git add . && git commit -m 'fix: saving' && git push",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.7.15",
    "@headlessui/react": "^1.6.6",
    "@metaplex-foundation/js": "^0.14.0",
    "@solana/web3.js": "^1.50.1",
    "clsx": "^1.1.1",
    "jest-dom": "^4.0.0",
    "jest-localstorage-mock": "^2.4.22",
    "next": "^12.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "tailwind-merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-jest": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.9.12",
    "postcss": "^8.4.12",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "standard-version": "^9.3.2",
    "storybook": "^6.5.9",
    "storybook-addon-next": "^1.6.7",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
